<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Shop.Shop">

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TableRow
            android:id="@+id/tableRow"
            android:layout_width="match_parent"
            android:layout_height="50dp">

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="198dp"
                android:layout_height="match_parent"
                android:layout_weight=".5"
                app:srcCompat="@drawable/logo" />

            <Button
                android:id="@+id/button"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:onClick="Admin"
                android:text="ADMIN" />

            <ImageButton
                android:id="@+id/imageButton"
                style="@android:style/Widget.Material.Light.ImageButton"
                android:layout_width="50dp"
                android:layout_height="match_parent"
                android:layout_weight="0"
                android:background="@android:color/transparent"
                android:elevation="0dp"
                android:onClick="Compute"
                android:padding="15dp"
                android:scaleType="centerInside"
                app:srcCompat="@drawable/cart" />

            <ImageButton
                android:id="@+id/imageButton8"
                android:layout_width="50dp"
                android:layout_height="wrap_content"
                android:background="@android:color/transparent"
                android:onClick="SignOut"
                android:padding="15dp"
                android:scaleType="centerInside"
                app:srcCompat="@drawable/logout" />

        </TableRow>

        <ScrollView
            android:id="@+id/scrollView2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:layout_editor_absoluteY="116dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <CheckBox
                        android:id="@+id/item1"
                        android:layout_width="30dp"
                        android:layout_height="match_parent"
                        android:text="DANKS ORIGINAL ROLLING PAPERS KING SIZE (25 PACKS)" />

                    <ImageButton
                        android:id="@+id/pr1"
                        android:layout_width="250dp"
                        android:layout_height="150dp"
                        android:background="@android:color/transparent"
                        android:onClick="OpenP1"
                        android:scaleType="centerCrop"
                        app:srcCompat="@drawable/p1" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/p1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="1100"
                            android:textAlignment="center"
                            android:textSize="24sp" />

                        <EditText
                            android:id="@+id/q1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:ems="10"
                            android:inputType="number"
                            android:text="1"
                            android:textAlignment="center"
                            android:textSize="30sp" />

                        <TextView
                            android:id="@+id/textView7"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Quantity"
                            android:textAlignment="center"
                            android:textSize="18sp" />

                    </LinearLayout>

                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <CheckBox
                        android:id="@+id/item2"
                        android:layout_width="30dp"
                        android:layout_height="match_parent"
                        android:text="DANKS NATURAL ROLLING PAPERS KING SIZE (25 PACKS)" />

                    <ImageButton
                        android:id="@+id/pr2"
                        android:layout_width="250dp"
                        android:layout_height="150dp"
                        android:background="@android:color/transparent"
                        android:onClick="OpenP1"
                        android:scaleType="centerCrop"
                        app:srcCompat="@drawable/p2" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/p2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="1400"
                            android:textAlignment="center"
                            android:textSize="24sp" />

                        <EditText
                            android:id="@+id/q2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:ems="10"
                            android:inputType="number"
                            android:text="1"
                            android:textAlignment="center"
                            android:textSize="30sp" />

                        <TextView
                            android:id="@+id/textView7"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Quantity"
                            android:textAlignment="center"
                            android:textSize="18sp" />

                    </LinearLayout>

                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <CheckBox
                        android:id="@+id/item3"
                        android:layout_width="30dp"
                        android:layout_height="match_parent"
                        android:text="JUICY STRAWBERRY FIELDS HEMP WRAPS (25 PACKS)" />

                    <ImageButton
                        android:id="@+id/pr3"
                        android:layout_width="250dp"
                        android:layout_height="150dp"
                        android:background="@android:color/transparent"
                        android:onClick="OpenP1"
                        android:scaleType="centerCrop"
                        app:srcCompat="@drawable/p3" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/p3"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="800"
                            android:textAlignment="center"
                            android:textSize="24sp" />

                        <EditText
                            android:id="@+id/q3"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:ems="10"
                            android:inputType="number"
                            android:text="1"
                            android:textAlignment="center"
                            android:textSize="30sp" />

                        <TextView
                            android:id="@+id/textView7"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Quantity"
                            android:textAlignment="center"
                            android:textSize="18sp" />

                    </LinearLayout>

                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <CheckBox
                        android:id="@+id/item4"
                        android:layout_width="30dp"
                        android:layout_height="match_parent"
                        android:text="JUICY NATURAL HEMP WRAPS (25 PACKS)" />

                    <ImageButton
                        android:id="@+id/pr4"
                        android:layout_width="250dp"
                        android:layout_height="150dp"
                        android:background="@android:color/transparent"
                        android:onClick="OpenP1"
                        android:scaleType="centerCrop"
                        app:srcCompat="@drawable/p4" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/p4"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="800"
                            android:textAlignment="center"
                            android:textSize="24sp" />

                        <EditText
                            android:id="@+id/q4"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:ems="10"
                            android:inputType="number"
                            android:text="1"
                            android:textAlignment="center"
                            android:textSize="30sp" />

                        <TextView
                            android:id="@+id/textView7"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Quantity"
                            android:textAlignment="center"
                            android:textSize="18sp" />

                    </LinearLayout>

                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <CheckBox
                        android:id="@+id/item5"
                        android:layout_width="30dp"
                        android:layout_height="match_parent"
                        android:text="JUICY JAYS WATERMELON ROLLING PAPERS (24 PACKS)" />

                    <ImageButton
                        android:id="@+id/pr5"
                        android:layout_width="250dp"
                        android:layout_height="150dp"
                        android:background="@android:color/transparent"
                        android:onClick="OpenP1"
                        android:scaleType="centerCrop"
                        app:srcCompat="@drawable/p5" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/p5"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="1500"
                            android:textAlignment="center"
                            android:textSize="24sp" />

                        <EditText
                            android:id="@+id/q5"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:ems="10"
                            android:inputType="number"
                            android:text="1"
                            android:textAlignment="center"
                            android:textSize="30sp" />

                        <TextView
                            android:id="@+id/textView7"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Quantity"
                            android:textAlignment="center"
                            android:textSize="18sp" />

                    </LinearLayout>

                </TableRow>

            </LinearLayout>
        </ScrollView>

    </LinearLayout>

</android.support.constraint.ConstraintLayout>